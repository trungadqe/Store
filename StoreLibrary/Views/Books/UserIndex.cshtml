								<a class="tg-btn" asp-controller = "Books" asp-action = "CusIndex">View All</a>
							</div>
						</div>
						<div id="tg-pickedbyauthorslider" class="tg-pickedbyauthor tg-pickedbyauthorslider owl-carousel">
							@foreach (var book in Model){
								<div class="item">
								<div class="tg-postbook">
									<figure class="tg-featureimg">
										<div class="tg-bookimg">
											<div class="tg-frontcover"><img src="@book.ImgUrl" alt="image description"></div>
										</div>
										<div class="tg-hovercontent">
											<div class="tg-description">
												<p>@book.Desc</p>
											</div>
											<strong class="tg-bookcategory">Category: @book.Category</strong>
											<strong class="tg-bookcategory">Pages: @book.Pages </strong>
											<strong class="tg-bookprice">Price: @book.Price</strong>											
										</div>
									</figure>
									<div class="tg-postbookcontent">
										<div class="tg-booktitle">
											<h3><a href="javascript:void(0);">@book.Title</a></h3>
										</div>
										<span class="tg-bookwriter"><a href="javascript:void(0);">By Author: @book.Author</a></span>
										<span class="tg-bookwriter"><a href="javascript:void(0);">By Store: @book.Store.Name</a></span>
										<a class="tg-btn tg-btnstyletwo" href="javascript:void(0);">
											<i class="fa fa-shopping-basket"></i>
											<em>Add To Basket</em>
										</a>
									</div>
								</div>
							</div>
							}
